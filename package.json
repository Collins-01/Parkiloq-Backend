{
  "name": "parkiloq-backend",
  "version": "1.0.0",
  "description": "Backend for Parkiloq parking space marketplace (Node.js, TypeScript, Clean Architecture, Supabase)",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "nodemon": "nodemon --watch src --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.7",
    "@types/uuid": "^10.0.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "stripe": "^18.3.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/nodemon": "^1.19.6",
    "@types/stripe": "^8.0.416",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
